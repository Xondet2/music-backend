{
  "version": 2,
  "builds": [
    {
      "src": "reproductor/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "reproductor/wsgi.py"
    }
  ]
}